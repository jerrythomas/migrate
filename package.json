{
  "name": "migrate",
  "version": "1.0.0-beta.0",
  "description": "",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "cover": "NODE_ENV=test nyc tap --no-coverage",
    "test": "standard && NODE_ENV=test tap --no-coverage",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -w -l info -P app.js",
    "cluster": "node -r dotenv/config cluster.js",
    "lint": "standard --fix",
    "codecov": "codecov",
    "snyk-protect": "snyk protect",
    "prepare": "pnpm run snyk-protect",
    "release": "release-it --preRelease=beta"
  },
  "tap": {
    "test-regex": ".*\\.(tests?|spec)\\.([mc]js|[jt]sx?)$",
    "nyc-arg": [
      "--exclude=zzz"
    ]
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "**/*.test.js",
      "tmp/**",
      "zzz/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jerrythomas/migrate.git"
  },
  "keywords": [],
  "author": "Jerry Thomas <me@jerrythomas.name>",
  "license": "ISC",
  "dependencies": {
    "bull": "^3.29.1",
    "config": "^3.3.6",
    "fastify": "^3.20.2",
    "fastify-autoload": "^3.8.1",
    "fastify-cli": "^3.0.0",
    "fastify-compress": "^3.6.0",
    "fastify-metrics": "^6.0.3",
    "fastify-mysql": "^2.1.0",
    "fastify-no-icon": "^4.0.0",
    "fastify-plugin": "^3.0.0",
    "fastify-postgres": "^3.6.0",
    "fastify-redis": "^4.3.1",
    "fastify-sensible": "^3.1.1",
    "fastify-swagger": "^3.5.0",
    "lodash": "^4.17.21",
    "node-jq": "^2.3.5",
    "pg": "^8.7.1",
    "snyk": "^1.695.0",
    "under-pressure": "^5.7.0"
  },
  "devDependencies": {
    "auto-changelog": "^2.3.0",
    "codecov": "^3.8.3",
    "dotenv": "^8.6.0",
    "ioredis": "^4.27.9",
    "nyc": "^15.1.0",
    "release-it": "^14.11.5",
    "rimraf": "^3.0.2",
    "standard": "^16.0.3",
    "tap": "^14.11.0"
  },
  "auto-changelog": {
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false
  },
  "snyk": true
}
