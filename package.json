{
  "name": "migrate",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "cover": "NODE_ENV=test nyc --exclude=zzz tap --no-coverage",
    "test": "standard && NODE_ENV=test tap --no-coverage",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -w -l info -P app.js",
    "cluster": "node -r dotenv/config cluster.js",
    "lint": "standard --fix",
    "codecov": "codecov",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "tap": {
    "test-regex": ".*\\.(tests?|spec)\\.([mc]js|[jt]sx?)$",
    "nyc-arg": [
      "--exclude=zzz"
    ]
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "**/*.test.js",
      "tmp/**",
      "zzz/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jerrythomas/migrate.git"
  },
  "keywords": [],
  "author": "Jerry Thomas <me@jerrythomas.name>",
  "license": "ISC",
  "dependencies": {
    "bull": "^3.13.0",
    "config": "^3.3.1",
    "fastify": "^3.9.2",
    "fastify-autoload": "^3.4.0",
    "fastify-cli": "^2.0.0",
    "fastify-compress": "^3.4.1",
    "fastify-metrics": "^6.0.3",
    "fastify-mysql": "^2.0.1",
    "fastify-no-icon": "^4.0.0",
    "fastify-plugin": "^3.0.0",
    "fastify-postgres": "^3.3.0",
    "fastify-redis": "^4.1.0",
    "fastify-sensible": "^3.1.0",
    "fastify-swagger": "^3.5.0",
    "lodash": "^4.17.16",
    "node-jq": "^1.11.0",
    "pg": "^8.0.3",
    "snyk": "^1.437.3",
    "under-pressure": "^5.4.0"
  },
  "devDependencies": {
    "codecov": "^3.6.5",
    "dotenv": "^8.2.0",
    "ioredis": "^4.16.3",
    "nyc": "^15.0.1",
    "rimraf": "^3.0.2",
    "standard": "^16.0.3",
    "tap": "^14.10.7"
  },
  "snyk": true
}
