{
  "name": "migrate",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "cover": "NODE_ENV=test nyc --exclude=zzz tap --no-coverage",
    "test": "standard && NODE_ENV=test tap --no-coverage",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -w -l info -P app.js",
    "cluster": "node -r dotenv/config cluster.js",
    "lint": "standard --fix",
    "codecov": "codecov"
  },
  "tap": {
    "test-regex": ".*\\.(tests?|spec)\\.([mc]js|[jt]sx?)$",
    "nyc-arg": [
      "--exclude=zzz"
    ]
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "**/*.test.js",
      "tmp/**",
      "zzz/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jerrythomas/migrate.git"
  },
  "keywords": [],
  "author": "Jerry Thomas <me@jerrythomas.name>",
  "license": "ISC",
  "dependencies": {
    "ajv": "^6.12.2",
    "bull": "^3.13.0",
    "config": "^3.3.1",
    "fastify": "^2.14.0",
    "fastify-autoload": "^1.0.0",
    "fastify-cli": "^1.5.0",
    "fastify-compress": "^2.0.1",
    "fastify-metrics": "^5.0.1",
    "fastify-mysql": "^1.0.1",
    "fastify-no-icon": "^3.0.0",
    "fastify-plugin": "^1.5.0",
    "fastify-postgres": "^2.0.0",
    "fastify-redis": "^3.3.1",
    "fastify-sensible": "^2.1.1",
    "fastify-swagger": "^2.5.1",
    "lodash": "^4.17.16",
    "node-jq": "^1.11.0",
    "pg": "^8.0.3",
    "under-pressure": "^3.2.1"
  },
  "devDependencies": {
    "codecov": "^3.6.5",
    "dotenv": "^8.2.0",
    "ioredis": "^4.16.3",
    "nyc": "^15.0.1",
    "rimraf": "^3.0.2",
    "standard": "^14.3.3",
    "tap": "^14.10.7"
  }
}
